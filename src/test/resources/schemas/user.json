{
  "xml": {
    "name": "User"
  }, 
  "$schema": "http://json-schema.org/schema#", 
  "type": "object", 
  "properties": {
    "username": {
      "type": "string"
    }, 
    "firstName": {
      "type": "string"
    }, 
    "lastName": {
      "type": "string"
    }, 
    "userStatus": {
      "type": "integer", 
      "description": "User Status", 
      "format": "int32"
    }, 
    "email": {
      "type": "string"
    }, 
    "phone": {
      "type": "string"
    }, 
    "password": {
      "type": "string"
    }, 
    "id": {
      "type": "integer", 
      "format": "int64"
    }
  }
}